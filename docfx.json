{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "_appLogoPath": "./Content_Portal/Documentation/images/atlas_icon.png",
      "_appName": "OSIsoft Cloud Services Documentation"
    }
  ],
  "build": {
	"globalMetadata": {
		"_enableSearch": true
	},
   "content":[
   {
      "files":[
         "Content_Portal/**/**.md",
         "Content_Portal/**/toc.yml",
         "toc.yml",
         "**.md"
      ],
      "exclude":[
         "Content_Portal/Documentation/Identity/Identity_ImplicitClient.md"
      ]
   }
],
    "resource": [
      {
        "files": [
            "Content_Portal/Quickstarts/images/**",
            "Content_Portal/Documentation/images/**",
            "Content_Portal/How_To_Guides/images/**",
	    "Content_Portal/Overview/images/**"	
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "_exported_templates/default"
    ],
    "postProcessors": ["OcsVariableProcessor"],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
