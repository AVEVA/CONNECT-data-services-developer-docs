{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "_appLogoPath": "./Documentation/images/atlas_icon.png",
      "_appName": "OSIsoft Cloud Services Documentation"
    }
  ],
  "build": {
	"globalMetadata": {
		"_enableSearch": true
	},
    "content": [
      {
        "files": [
          "Documentation/**/toc.yml",
          "Documentation/**/**.md",
          "Documentation/toc.yml",
          "toc.yml",
          "**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "Documentation/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": ["OcsVariableProcessor"],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}