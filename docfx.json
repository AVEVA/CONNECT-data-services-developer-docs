{
	"metadata": [
		{
			"src": [
				{
					"files": ["src/**.csproj"]
				}
			],
			"dest": "api",
			"disableGitFeatures": false,
			"_appLogoPath": "./content/api-reference/images/atlas_icon.png",
			"_appName": "CONNECT data services Developer Documentation"
		}
	],
	"build": {
		"globalMetadata": {
			"_enableSearch": true,
			"_gitContribute": {
				"branch": "main"
			}
		},
		"content": [
			{
				"files": [
					"content/**/**.md",
					"content/**/toc.yml",
					"toc.yml",
					"**.md"
				],
				"exclude": [
					"content/api-reference/identity/identity-implicit-clients.md",
					"content/external-references/**.yaml"
				]
			}
		],
		"resource": [
			{
				"files": [
					"content/_images/**",
					"content/api-reference/images/**",
					"content/developer-guide/images/**",
					"content/_icons/**"
				]
			}
		],
		"overwrite": [
			{
				"files": ["apidoc/**.md"],
				"exclude": ["obj/**", "_site/**"]
			}
		],
		"dest": "_site",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": ["default", "modern"],
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": true,
		"disableGitFeatures": false
	}
}
